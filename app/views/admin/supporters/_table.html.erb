<div id="supporters">
  <table>
    <thead>
      <tr> <th>ID</th> <th>Name</th> <th>Email</th> <th>Is Member?</th> </tr>
    </thead>
    <tbody>
      <% @supporters.find_each do |supporter| %>
        <tr>
          <td><%= supporter.id %></td>
          <td><%= supporter.name %></td>
          <td><%= supporter.email %></td>
          <td><%= supporter.is_member %></td>
        </tr>
      <% end %>
    </tbody>
  </table>
</div>
